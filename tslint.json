{
  "defaultSeverity": "error",
  "linterOptions": {
    "exclude": [
      "src/serviceWorker.ts"
    ]
  },
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "ordered-imports": true
  },
  "rulesDirectory": []
}
